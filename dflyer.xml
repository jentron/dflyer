<?xml version="1.0"?>
<?xml-stylesheet type="text/xsl" href="http://jsbsim.sourceforge.net/JSBSim.xsl"?>
<fdm_config name="DA20-A1" version="2.0" release="ALPHA"
   xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
   xsi:noNamespaceSchemaLocation="http://jsbsim.sourceforge.net/JSBSim.xsd">

 <fileheader>
  <author> Ron Jensen </author>
  <filecreationdate> 2009-01-23 </filecreationdate>
  <version>0.01 </version>
  <description> Models a four rotor UAV similar to the Draganflyer XPro. </description>
 </fileheader>

 <metrics>
   <wingarea  unit="M2">   0.017</wingarea>
   <wingspan  unit="M" >   0.13 </wingspan>
   <chord     unit="FT" >  0.0 </chord>
   <htailarea unit="FT2">  0.0 </htailarea>
   <htailarm  unit="FT" >  0.0 </htailarm>
   <vtailarea unit="FT2">  0.0 </vtailarea>
   <vtailarm  unit="FT" >  0.0 </vtailarm>
   <location name="AERORP" unit="IN">
     <x>   0.00 </x>
     <y>   0.00 </y>
     <z>   0.00 </z>
   </location>
   <location name="EYEPOINT" unit="IN">
     <x>  0.0 </x>
     <y>  0.0 </y>
     <z>  0.0 </z>
   </location>
   <location name="VRP" unit="IN">
     <x>0</x>
     <y>0</y>
     <z>0</z>
   </location>
 </metrics>

 <mass_balance>
   <ixx unit="KG*M2">  7.5 </ixx>
   <iyy unit="KG*M2"> 74.0 </iyy>
   <izz unit="KG*M2"> 74.0 </izz>
   <ixy unit="KG*M2">  0.0 </ixy>
   <ixz unit="KG*M2">  0.0 </ixz>
   <iyz unit="KG*M2">  0.0 </iyz>
   <emptywt unit="KG"> 2.000 </emptywt>
   <location name="CG" unit="IN">
     <x>  0.0 </x>
     <y>  0.0 </y>
     <z>  0.0 </z>
   </location>
   <pointmass name="Payload">
     <weight unit="KG"> 0 </weight>
     <location unit="IN">
       <x>  0.0 </x>
       <y>  0.0 </y>
       <z>  0.0 </z>
     </location>
   </pointmass> 
 </mass_balance>

 <ground_reactions>

  <contact type="STRUCTURE" name="frontbase">
   <location unit="M">
    <x>  -0.283 </x>
    <y>   0.00 </y>
    <z>  -0.033</z>
   </location>
   <static_friction>  0.80 </static_friction>
   <dynamic_friction> 0.50 </dynamic_friction>
   <spring_coeff unit="N/M">       200 </spring_coeff>
   <damping_coeff unit="N/M/SEC">  20 </damping_coeff>
  </contact>

  <contact type="STRUCTURE" name="rearbase">
   <location unit="M">
    <x>   0.283 </x>
    <y>   0.00 </y>
    <z>  -0.033</z>
   </location>
   <static_friction>  0.80 </static_friction>
   <dynamic_friction> 0.50 </dynamic_friction>
   <spring_coeff unit="N/M">       200 </spring_coeff>
   <damping_coeff unit="N/M/SEC">  20 </damping_coeff>
  </contact>

  <contact type="STRUCTURE" name="leftbase">
   <location unit="M">
    <x>   0.00 </x>
    <y>  -0.283 </y>
    <z>  -0.033</z>
   </location>
   <static_friction>  0.80 </static_friction>
   <dynamic_friction> 0.50 </dynamic_friction>
   <spring_coeff unit="N/M">       200 </spring_coeff>
   <damping_coeff unit="N/M/SEC">  20 </damping_coeff>
  </contact>

  <contact type="STRUCTURE" name="rightbase">
   <location unit="M">
    <x>   0.00 </x>
    <y>   0.283 </y>
    <z>  -0.033</z>
   </location>
   <static_friction>  0.80 </static_friction>
   <dynamic_friction> 0.50 </dynamic_friction>
   <spring_coeff unit="N/M">       200 </spring_coeff>
   <damping_coeff unit="N/M/SEC">  20 </damping_coeff>
  </contact>

 </ground_reactions>

<!-- the front and rear motors spin clockwise, and the left and right motors spin counter-clockwise. -->
 <propulsion>
  <engine file="eng_150w" name="front">
   <location unit="M">
    <x>   0.283 </x>
    <y>   0.00  </y>
    <z>   0.00  </z>
   </location>
   <orient unit="DEG">
    <pitch> 90.00 </pitch>
    <roll>   0.00 </roll>
    <yaw>    0.00 </yaw>
   </orient>
   <feed>0</feed>
   <thruster file="18x8">
    <location unit="M">
     <x>   0.283 </x>
     <y>   0.00  </y>
     <z>   0.025 </z>
    </location>
    <orient unit="DEG">
     <pitch> 90.00 </pitch>
     <roll>   0.00 </roll>
     <yaw>    0.00 </yaw>
    </orient>
    <sense> 1 </sense>
    <p_factor> 1 </p_factor>
   </thruster>
  </engine>

  <engine file="eng_150w" name="rear">
   <location unit="M">
    <x>  -0.283 </x>
    <y>   0.000 </y>
    <z>   0.000 </z>
   </location>
   <orient unit="DEG">
    <pitch> 90.00 </pitch>
    <roll>   0.00 </roll>
    <yaw>    0.00 </yaw>
   </orient>
   <feed>0</feed>
   <thruster file="18x8">
    <location unit="M">
     <x>  -0.283 </x>
     <y>   0.000 </y>
     <z>   0.025 </z>
    </location>
    <orient unit="DEG">
     <pitch> 90.00 </pitch>
     <roll>   0.00 </roll>
     <yaw>    0.00 </yaw>
    </orient>
    <sense> 1 </sense>
    <p_factor> 1 </p_factor>
   </thruster>
  </engine>

  <engine file="eng_150w" name="left">
   <location unit="M">
    <x>   0.00 </x>
    <y>   0.283 </y>
    <z>   0.00 </z>
   </location>
   <orient unit="DEG">
    <pitch> 90.00 </pitch>
    <roll>   0.00 </roll>
    <yaw>    0.00 </yaw>
   </orient>
   <feed>0</feed>
   <thruster file="18x8">
    <location unit="M">
     <x>   0.00 </x>
     <y>   0.283 </y>
     <z>   0.025 </z>
    </location>
    <orient unit="DEG">
     <pitch> 90.00 </pitch>
     <roll>   0.00 </roll>
     <yaw>    0.00 </yaw>
    </orient>
    <sense> -1 </sense>
    <p_factor> 1 </p_factor>
   </thruster>
  </engine>

  <engine file="eng_150w" name="right">
   <location unit="M">
    <x>   0.000 </x>
    <y>  -0.283 </y>
    <z>   0.000 </z>
   </location>
   <orient unit="DEG">
    <pitch> 90.00 </pitch>
    <roll>   0.00 </roll>
    <yaw>    0.00 </yaw>
   </orient>
   <feed>0</feed>
   <thruster file="18x8">
    <location unit="M">
     <x>   0.000 </x>
     <y>  -0.283 </y>
     <z>   0.025 </z>
    </location>
    <orient unit="DEG">
     <pitch> 90.00 </pitch>
     <roll>   0.00 </roll>
     <yaw>    0.00 </yaw>
    </orient>
    <sense> -1 </sense>
    <p_factor> 1 </p_factor>
   </thruster>
  </engine>

  <tank type="FUEL" number="0">
     <location unit="M">
       <x>   0.00 </x>
       <y>   0.00 </y>
       <z>  -0.07 </z>
     </location>
     <capacity unit="KG"> 0.480 </capacity>
     <contents unit="KG"> 0.480 </contents>
  </tank>

 </propulsion>

 <aerodynamics>
  <axis name="LIFT">
  </axis>
  <axis name="DRAG">
    <function name="aero/coefficient/CD0">
       <description>Overall Drag</description>
       <product>
           <property>aero/qbar-psf</property>
           <property>metrics/Sw-sqft</property>
           <value>1</value>
       </product>
    </function>
  </axis>
  <axis name="SIDE">
  </axis>
  <axis name="ROLL">
  </axis>
  <axis name="PITCH">
  </axis>
  <axis name="YAW">
  </axis>
 </aerodynamics>

</fdm_config>
